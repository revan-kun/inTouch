<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Simple Registration</title>
</head>
<body>
<form method="post">
	<br> Name: <input type="text" name="memberName">
	<br> Surname: <input type="text" name="memberSurname">
	<br> Login (email): <input type="text" name="memberLogin">
	<br> Password: <input type="text" name="memberPassword">
	<br> Birthday: <input type="text" name="memberBirthday">
	<br> Registration date: <input type="text" name="memberRegistrationDate">
	<br> Gender: <input type="text" name="memberSex">
	<br> Qualification level: <input type="text" name="memberQualification">
	<br> Experience: <input type="text" name="memberExperience">
	<br> Photo (link): <input type="text" name="memberPhoto">
	<br> Profession: <input type="text" name="memberProjectRole">
	<br> <input type="submit" value="Submit!">
</form>
</body>
</html>